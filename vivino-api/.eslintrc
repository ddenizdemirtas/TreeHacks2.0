{
	"env": {
	  "browser": true,
	  "node": true,
	  "es6": true
	},
	"extends": ["eslint:recommended", "prettier"],
	"plugins": ["prettier"],
	"parserOptions": {
	  "ecmaVersion": 2019,
	  "sourceType": "module"
	},
	"rules": {
	  "prettier/prettier": "error",
	  "indent": ["error", "tab", { "SwitchCase": 1 }],
	  "linebreak-style": ["error", "unix"],
	  "quotes": ["error", "single"],
	  "semi": ["error", "always"]
	}
  }
  